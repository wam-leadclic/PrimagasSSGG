@isTest
private class TEST_WSEnviarImagenes
{	
	@isTest static void peticionEnviarConDatosIncorrectos() 
	{
		WSEnviarImagenes.PeticionImagen peticion = new WSEnviarImagenes.PeticionImagen();
		peticion.datos = null;

		Test.startTest();
		String jDatos = WSEnviarImagenes.enviar(peticion);
		Test.stopTest();

		WSEnviarImagenes.Respuesta r = (WSEnviarImagenes.Respuesta) JSON.deserialize(jDatos, WSEnviarImagenes.Respuesta.class);

		System.debug('Respuesta: ' + r);

		System.assertEquals('101', r.resultado.codigo);
	}

	@isTest static void peticionEnviarConDatosCorrectos() 
	{
		WSEnviarImagenes.PeticionImagen peticion = new WSEnviarImagenes.PeticionImagen();
		peticion.datos = new WSEnviarImagenes.Datos();

		//Creo la imagen
		Document imagen = new Document();
		imagen.Name = 'Imagen prueba';
		imagen.DeveloperName = 'Imagen_prueba';
		String b64 = '';
		Blob body = EncodingUtil.base64Decode(b64);
		imagen.Body = body;
		imagen.FolderId = UserInfo.getUserId();
		insert imagen;

		//Creo el registro en la tabla maestra
		ImagenesOferta__c imagenOferta = new ImagenesOferta__c();
		imagenOferta.Name = 'ImagenOfertaPrueba';
		imagenOferta.ImagenBase64__c = imagen.DeveloperName;
		insert imagenOferta;

		peticion.datos.idSalesForce = imagenOferta.Id;

		Test.startTest();
		String jDatos = WSEnviarImagenes.enviar(peticion);
		Test.stopTest();

		WSEnviarImagenes.Respuesta r = (WSEnviarImagenes.Respuesta) JSON.deserialize(jDatos, WSEnviarImagenes.Respuesta.class);

		System.debug('Respuesta: ' + r);

		System.assertEquals('100', r.resultado.codigo);
		System.assertEquals(b64, r.imagen.imagenBase64);
	}
}